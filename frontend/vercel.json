{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "app.infrascribe.dev"
        }
      ],
      "destination": "/app",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "/index.html"
    }
  ]
}
